{
  "name": "Chrome File Server",
  "version": "1.0",
  "description": "Description",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts" : ["js/background.js"]
    }
  },
  "permissions" : [
	  "<all_urls>",
	  "storage",
	  {
		  "sockets" : {
		    "tcpServer" : {
		      "listen" : ""
		    },
		    "tcp" : {
		      "connect" : ""
		    }
		  }
	  },
	  {
	    "fileSystem" : ["directory", "retainEntries"]
	  }
  ]
}