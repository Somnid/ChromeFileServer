{
  "name": "Chrome File Server",
  "version": "1.0",
  "description": "Description",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts" : ["js/background.js"]
    }
  },
  "permissions" : [
	  "<all_urls>",
	  "storage",
	  {
		  "socket" : ["tcp-connect", "tcp-listen"]
	  },
	  {
	    "fileSystem" : ["directory", "retainEntries"]
	  }
  ]
}